{
  "name": "stormee-electron-react-three",
  "version": "0.1.0",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev:renderer": "vite",
    "dev:preload": "esbuild electron/preload.ts --bundle --platform=node --outfile=dist-electron/preload.cjs --external:electron --format=cjs --sourcemap --watch",
    "dev:electron": "tsc -p tsconfig.electron.json --watch",
    "dev": "concurrently -k -n renderer,preload,main,electron -c magenta,cyan,blue,green \"vite\" \"npm:dev:preload\" \"npm:dev:electron\" \"cross-env ELECTRON_RENDERER_URL=http://localhost:5173 wait-on tcp:5173 file:dist-electron/main.js file:dist-electron/preload.cjs && electron .\"",
    "build:renderer": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json && esbuild electron/preload.ts --bundle --platform=node --outfile=dist-electron/preload.cjs --external:electron --format=cjs",
    "build": "npm run build:renderer && npm run build:electron",
    "start": "electron .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.168.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^30.0.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "wait-on": "^7.0.1"
  }
}
